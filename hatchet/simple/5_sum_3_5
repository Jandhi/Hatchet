print 'Enter a number'

num = read

while int n == None 
    print 'Enter a number'
    num = read

// filtered with lambda
1...n | filter (x) => {x % 3 == 0 or x % 5 == 0} | sum | print

// filtered with fill-in
1...n | filter (0 in ? % 3, ? % 5) | sum | print

// where 
1...n where (x) => {x % 3 == 0, x % 5 == 0} | sum | print

